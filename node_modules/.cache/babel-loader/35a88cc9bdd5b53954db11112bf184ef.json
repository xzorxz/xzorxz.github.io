alidate email\n * @param email\n * @returns {boolean}\n */\nexport function validateEmail (email) {\n  // eslint-disable-next-line\n  const re = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/\n  return re.test(email)\n}\n"],"mappings":";;;AAAA;AACA;AACA;;AAEA,OAAO,SAASA,eAAe,CAAEC,GAAG,EAAE;EACpC,IAAMC,QAAQ,GAAG,CAAC,OAAO,EAAE,QAAQ,CAAC;EACpC,OAAOA,QAAQ,CAACC,OAAO,CAACF,GAAG,CAACG,IAAI,EAAE,CAAC,IAAI,CAAC;AAC1C;;AAEA;AACA,OAAO,SAASC,WAAW,CAAEC,OAAO,EAAE;EACpC,IAAMC,QAAQ,GAAG,4TAA4T;EAC7U,OAAOA,QAAQ,CAACC,IAAI,CAACF,OAAO,CAAC;AAC/B;;AAEA;AACA,OAAO,SAASG,iBAAiB,CAAER,GAAG,EAAE;EACtC,IAAMS,GAAG,GAAG,UAAU;EACtB,OAAOA,GAAG,CAACF,IAAI,CAACP,GAAG,CAAC;AACtB;;AAEA;AACA,OAAO,SAASU,iBAAiB,CAAEV,GAAG,EAAE;EACtC,IAAMS,GAAG,GAAG,UAAU;EACtB,OAAOA,GAAG,CAACF,IAAI,CAACP,GAAG,CAAC;AACtB;;AAEA;AACA,OAAO,SAASW,gBAAgB,CAAEX,GAAG,EAAE;EACrC,IAAMS,GAAG,GAAG,aAAa;EACzB,OAAOA,GAAG,CAACF,IAAI,CAACP,GAAG,CAAC;AACtB;;AAEA;AACA;AACA;AACA;AACA;AACA,OAAO,SAASY,aAAa,CAAEC,KAAK,EAAE;EACpC;EACA,IAAMC,EAAE,GAAG,yJAAyJ;EACpK,OAAOA,EAAE,CAACP,IAAI,CAACM,KAAK,CAAC;AACvB"}]}