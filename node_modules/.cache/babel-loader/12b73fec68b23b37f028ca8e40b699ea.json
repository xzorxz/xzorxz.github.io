eft\n  const top = height / 2 - h / 2 + dualScreenTop\n  const newWindow = window.open(\n    url,\n    title,\n    'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=yes, copyhistory=no, width=' +\n      w +\n      ', height=' +\n      h +\n      ', top=' +\n      top +\n      ', left=' +\n      left\n  )\n\n  // Puts focus on the newWindow\n  if (window.focus) {\n    newWindow.focus()\n  }\n}\n"],"mappings":"AAAA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,eAAe,SAASA,UAAU,CAAEC,GAAG,EAAEC,KAAK,EAAEC,CAAC,EAAEC,CAAC,EAAE;EACpD;EACA,IAAMC,cAAc,GAClBC,MAAM,CAACC,UAAU,KAAKC,SAAS,GAAGF,MAAM,CAACC,UAAU,GAAGE,MAAM,CAACC,IAAI;EACnE,IAAMC,aAAa,GACjBL,MAAM,CAACM,SAAS,KAAKJ,SAAS,GAAGF,MAAM,CAACM,SAAS,GAAGH,MAAM,CAACI,GAAG;EAEhE,IAAMC,KAAK,GAAGR,MAAM,CAACS,UAAU,GAC3BT,MAAM,CAACS,UAAU,GACjBC,QAAQ,CAACC,eAAe,CAACC,WAAW,GAClCF,QAAQ,CAACC,eAAe,CAACC,WAAW,GACpCT,MAAM,CAACK,KAAK;EAClB,IAAMK,MAAM,GAAGb,MAAM,CAACc,WAAW,GAC7Bd,MAAM,CAACc,WAAW,GAClBJ,QAAQ,CAACC,eAAe,CAACI,YAAY,GACnCL,QAAQ,CAACC,eAAe,CAACI,YAAY,GACrCZ,MAAM,CAACU,MAAM;EAEnB,IAAMT,IAAI,GAAGI,KAAK,GAAG,CAAC,GAAGX,CAAC,GAAG,CAAC,GAAGE,cAAc;EAC/C,IAAMQ,GAAG,GAAGM,MAAM,GAAG,CAAC,GAAGf,CAAC,GAAG,CAAC,GAAGO,aAAa;EAC9C,IAAMW,SAAS,GAAGhB,MAAM,CAACiB,IAAI,CAC3BtB,GAAG,EACHC,KAAK,EACL,sHAAsH,GACpHC,CAAC,GACD,WAAW,GACXC,CAAC,GACD,QAAQ,GACRS,GAAG,GACH,SAAS,GACTH,IAAI,CACP;;EAED;EACA,IAAIJ,MAAM,CAACkB,KAAK,EAAE;IAChBF,SAAS,CAACE,KAAK,EAAE;EACnB;AACF"}]}